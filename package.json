{
  "name": "multimonitorcalculator",
  "version": "3.1.0",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme/light",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/typography": "^14.0.0",
    "@neodrag/svelte": "^1.1.4",
    "@smui/button": "^6.0.0-beta.16",
    "@smui/card": "^6.0.0-beta.16",
    "@smui/checkbox": "^6.0.0-beta.16",
    "@smui/circular-progress": "^6.0.0-beta.16",
    "@smui/common": "^6.0.0-beta.16",
    "@smui/data-table": "^6.0.0-beta.16",
    "@smui/dialog": "^6.0.0-beta.16",
    "@smui/drawer": "^6.0.0-beta.16",
    "@smui/fab": "^6.0.0-beta.16",
    "@smui/form-field": "^6.0.0-beta.16",
    "@smui/menu-surface": "^6.0.0-beta.16",
    "@smui/paper": "^6.0.0-beta.16",
    "@smui/radio": "^6.0.0-beta.16",
    "@smui/segmented-button": "^6.0.0-beta.16",
    "@smui/select": "^6.0.0-beta.16",
    "@smui/slider": "^6.0.0-beta.16",
    "@smui/snackbar": "^6.0.0-beta.16",
    "@smui/switch": "^6.0.0-beta.16",
    "@smui/tab": "^6.0.0-beta.16",
    "@smui/tab-bar": "^6.0.0-beta.16",
    "@smui/textfield": "^6.0.0-beta.16",
    "@smui/tooltip": "^6.0.0-beta.16",
    "@smui/top-app-bar": "^6.0.0-beta.16",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-netlify": "1.0.0-next.49",
    "@sveltejs/kit": "1.0.0-next.291",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "query-string": "^7.1.1",
    "smui-theme": "^6.0.0-beta.16",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.0",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "uuid": "^8.3.2"
  },
  "type": "module"
}
